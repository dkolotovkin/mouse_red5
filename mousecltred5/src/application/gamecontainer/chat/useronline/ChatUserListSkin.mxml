<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:pr="com.primitives.*" xmlns:fx="http://ns.adobe.com/mxml/2009"  xmlns:s="library://ns.adobe.com/flex/spark">
    <s:states>
        <s:State name="normal" />
        <s:State name="over" />
        <s:State name="down" />
    </s:states>
	
	<s:Rect width="100%" height="100%">
		<s:stroke>
			<s:SolidColorStroke color="0xb7b7b7" weight="1" pixelHinting= "true" alpha="0"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="0xF7F7F7" alpha="0"/>
		</s:fill>
	</s:Rect>
	
	<s:Group id="contentGroup" width="100%" height="100%" >
        <s:layout>
            <s:BasicLayout />
        </s:layout>
    </s:Group>
</s:SparkSkin> 