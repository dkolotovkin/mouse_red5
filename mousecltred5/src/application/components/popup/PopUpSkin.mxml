<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"  xmlns:s="library://ns.adobe.com/flex/spark" xmlns:mx="library://ns.adobe.com/flex/mx">
	    <s:states>
        <s:State name="normal" />
        <s:State name="disabled" /> 
    </s:states>
     
     
	 <s:Rect width="100%" height="100%" radiusX="4" radiusY="4">
    	<s:fill>
    		<s:SolidColor color="0xFFFFFF" />
    	</s:fill>
    </s:Rect>
	
	<s:Group id="m" width="100%" height="100%">
		<s:Rect width="100%" height="100%" radiusX="4" radiusY="4" left="1" right="1" top="1" bottom="1">
			<s:fill>
				<s:SolidColor color="0xFFFFFF" />
			</s:fill>
		</s:Rect>		
	</s:Group>
	
	<s:BitmapImage source="{IconPopUpBg}" fillMode="repeat" width="100%" height="100%" left="1" right="1" top="1" bottom="1" mask="{m}"/>
	
	<s:Rect width="100%" height="100%" radiusX="4" radiusY="4" left="1" right="1" top="1" bottom="1">
		<s:fill>
			<s:RadialGradient>
				<s:entries>
					<s:GradientEntry color="0x000000" ratio="0.0" alpha="0"/>					
					<s:GradientEntry color="0x000000" ratio="1.0" alpha=".5"/>
				</s:entries>
			</s:RadialGradient>
		</s:fill>
	</s:Rect>	
	
    
    <s:Group id="contentGroup" left="8" right="8" top="8" bottom="8" > 
        <s:layout>
            <s:BasicLayout />
        </s:layout>
    </s:Group>
   <!--
   <s:Group id="maskGroup" width="100%" height="100%">
	   <s:Rect width="100%" height="100%" radiusX="8" radiusY="8">
	    	<s:fill>
	    		<s:SolidColor color="0x000000" />
	    	</s:fill>
	    </s:Rect>
   </s:Group>
    -->
</s:SparkSkin>